
<div class="boxed_wrapper">

        <!-- Page Title -->
        <section class="page-title">
            <div class="img-wrap parallax-demo-1">
                <div class="parallax-inner back-img" style="background-image: url(assets/images/background/page-title.jpg);"></div>
            </div>
            <div class="auto-container">
                <div class="content-box">
                    <ul class="bread-crumb clearfix">
                        <li><a href="index-2.html">Home</a></li>
                        <li>Our Animals</li>
                    </ul>
                    <div class="title">
                        <h1>Our Rides </h1>
                    </div>
                </div>
            </div>
        </section>
        <!-- End Page Title -->
        <% if (ride && ride.length > 0) { %>

           
        <section class="gallery-page-section">
            <div class="auto-container">
                <div class="row clearfix">
                    <% for (let i = 0; i < ride.length; i++) { %>
                        <% let currentride = ride[i]; %>
                    <div class="col-lg-4 col-md-6 col-sm-12 gallery-block">
                        <div class="gallery-block-two">
                            <div class="inner-box">
                                <figure class="image-box"><img src="<%=currentride.rideimage%>" alt=""><h3 style="text-align: center;"><%=currentride.ridename%></h3></figure>
                                <div class="view-box"><a href="<%=currentride.rideimage%>" class="lightbox-image" data-fancybox="gallery"><i class="flaticon-plus-symbol"></i></a></div>
                            </div>
                        </div>
                    </div>
                    <%}%>
                </div>
            </div>
          
        </section>
      
        <% } else { %>
            <h1>Admin Please add some Rides</h1>
        <%}%>


  </div>