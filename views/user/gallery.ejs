  
      <div class="boxed_wrapper">
  
  <!-- Page Title -->
     <section class="page-title">
        <div class="img-wrap parallax-demo-1">
            <div class="parallax-inner back-img" style="background-image: url(assets/images/background/page-title1.jpg);"></div>
        </div>
        <div class="auto-container">
            <div class="content-box">
                <ul class="bread-crumb clearfix">
                    <li><a href="/">Home</a></li>
                    <li>Our Gallery</li>
                </ul>
                <div class="title">
                    <h1>Our Gallery</h1>
                </div>
            </div>
        </div>
    </section>
    <!-- End Page Title -->


    <% if (gallerys && gallerys.length > 0) { %>
          <!-- gallery-page-section -->
    <section class="gallery-page-section">
        <div class="auto-container">
            <div class="row clearfix">
                <% for (let i = 0; i < gallerys.length; i++) { %>
                    <% let currentgallerys = gallerys[i]; %>
                <div class="col-lg-4 col-md-6 col-sm-12 gallery-block">
                    <div class="gallery-block-two">
                        <div class="inner-box">
                            <figure class="image-box "><img src="<%=currentgallerys.gallerysimage%>" alt="" ></figure>
                            <div class="view-box"><a href="<%=currentgallerys.gallerysimage%>" class="lightbox-image" data-fancybox="gallery"><i class="flaticon-plus-symbol"></i></a></div>
                        </div>
                    </div>
                </div>
             <%}%>
                
            </div>
        </div>
    </section>
    <!-- gallery-page-section end -->
    <% } else { %>
        <section class="gallery-page-section">
            <div class="auto-container">
                <div class="row clearfix">
                    <div class="col-lg-4 col-md-6 col-sm-12 gallery-block">
                        <div class="gallery-block-two">
                            <div class="inner-box">
                                <figure class="image-box"><img src="assets/images/gallery/gallery-1.jpg" alt=""></figure>
                                <div class="view-box"><a href="assets/images/gallery/gallery-1.jpg" class="lightbox-image" data-fancybox="gallery"><i class="flaticon-plus-symbol"></i></a></div>
                            </div>
                        </div>
                    </div>
                 
                    
                </div>
            </div>
        </section>
        <%}%>

      <!-- scroll to top -->
      <button class="scroll-top scroll-to-target" data-target="html">
        <i class="fal fa-long-arrow-up"></i>
    </button>
</div>